{
    "cli": {
      "version": ">= 16.17.4",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "API_BASE_URL": "https://crmdev.shoro.kg/api"
        }
      },
      "preview": {
        "distribution": "internal",
        "env": {
          "API_BASE_URL": "https://crmdev.shoro.kg/api"
        },
        "autoIncrement": true,
        "channel": "preview"
      },
      "production": {
        "autoIncrement": true,
        "env": {
          "API_BASE_URL": "https://crm.shoro.kg/api"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }